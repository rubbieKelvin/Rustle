{
    "yaml.schemas": {
        "http://json-schema.org/draft-07/schema": [
            "**/*.draft-07.schema.yaml"
        ],
        "https://raw.githubusercontent.com/rubbieKelvin/Rustle/refs/heads/main/api/src/schema/rustle-v0.0.1.draft-07.schema.yaml": [
            "**/*.api.yml",
            "**/*.api.yaml",
            "**/*.rt.yaml",
            "**/*.rt.yml"
        ],
    },
    "tailwindCSS.experimental.classRegex": [
        "class: \"(.*)\""
    ],
    "tailwindCSS.includeLanguages": {
        "rust": "html"
    },
}